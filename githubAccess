import json
import urllib3
#from github import Github

http = urllib3.PoolManager()
req = http.Request(GET, "https://api.github.com/repos/torvalds/linux/commits")
opener = urllib3.build_opener()
f = opener.open(req)
json = json.loads(f.read())

print("Jason")
print (json)

